    <select id="selectTProjectInfoList" parameterType="TProjectInfo" resultMap="TProjectInfoResult">
        <include refid="selectTProjectInfoVo"/>
        <where>  
            <if test="projectCode != null  and projectCode != ''"> and project_code = #{projectCode}</if>
            <if test="projectName != null  and projectName != ''"> and project_name like concat('%', #{projectName}, '%')</if>
            <if test="projectFullName != null  and projectFullName != ''"> and project_full_name like concat('%', #{projectFullName}, '%')</if>
            <if test="projectType != null  and projectType != ''"> and project_type = #{projectType}</if>
            <if test="projectKind != null  and projectKind != ''"> and project_kind = #{projectKind}</if>
            <if test="projectClass != null  and projectClass != ''"> and project_class = #{projectClass}</if>
            <if test="startDate != null "> and start_date = #{startDate}</if>
            <if test="endDate != null "> and end_date = #{endDate}</if>
            <if test="actEndDate != null "> and act_end_date = #{actEndDate}</if>
            <if test="status != null  and status != ''"> and status = #{status}</if>
            <if test="managerId != null  and managerId != ''"> and manager_id = #{managerId}</if>
            <if test="custManagerId != null  and custManagerId != ''"> and cust_manager_id = #{custManagerId}</if>
            <if test="progress != null "> and progress = #{progress}</if>
            <if test="budget != null "> and budget = #{budget}</if>
            <if test="preWorkloadByhour != null "> and pre_workload_byhour = #{preWorkloadByhour}</if>
            <if test="actWorkloadByhour != null "> and act_workload_byhour = #{actWorkloadByhour}</if>
        </where>
    </select>
    
    <select id="selectTProjectInfoById" parameterType="Long" resultMap="TProjectInfoResult">
