    <sql id="selectTProjectInfoVo">
        select id, project_code, project_name, project_full_name, project_type, project_kind, project_class, start_date, end_date, act_end_date, status, remark, manager_id, cust_manager_id, progress, budget, pre_workload_byhour, act_workload_byhour, create_time, update_time, create_by, update_by from t_project_info
